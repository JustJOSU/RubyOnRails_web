<div class="container">
  <div>
    <div>
          <div><h2>회원가입</h2></div>

          <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
            <!-- <%= render "devise/shared/error_messages", resource: resource %> -->
            <div class="field">
              <div><%= f.label :이메일 %></div>
              <div><%= f.email_field :email, autofocus: true, autocomplete: "email" %></div>
            </div>

            <div class="field">
              <div><%= f.label :비밀번호 %></div>
              <div><%= f.password_field :password, autocomplete: "new-password" %></div>
            </div>

            <div class="field">
              <div><%= f.label :비밀번호확인 %></div>
              <div><%= f.password_field :password_confirmation, autocomplete: "new-password" %></div>
            </div>

            <div class="field">
              <div><%= f.label :이름 %></div>
              <div><%= f.text_field :name, autocomplete: "name" %></div>
            </div>

            <div class="field">
              <div><%= f.label :나이 %></div>
              <div><%= f.text_field :age, autocomplete: "age" %></div>
            </div>

            <div class="field">
              <div><%= f.label :전화번호 %></div>
              <div><%= f.text_field :phone_n, autocomplete: "phone_n" %></div>
            </div>

            <div class="field">
              <div><%= f.label :주소 %></div>
              <div><%= f.text_field :address, autocomplete: "address" %></div>
            </div>  

            <div class="actions">
              <%= f.submit "가입하기" %>
            </div>
          <% end %>
    </div>
  </div>
</div>
    
<!-- <%= render "devise/shared/links" %> -->
